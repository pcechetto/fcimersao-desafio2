generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"
}


datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Asset {
  id     String @id @map("_id")
  symbol String
}

model Order {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  assetId  String
  price    Float
  status   OrderStatus @default(PENDING)
}

enum OrderStatus {
  //OPEN
  PENDING
  //CLOSED
}


